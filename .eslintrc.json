{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "array-bracket-spacing": [2, "always"],
        "no-const-assign": 2,
        "no-var": "error",
        "indent": [2, 2],
        "quotes": [2, "backtick"],
        "eqeqeq": "error"
    }
}
